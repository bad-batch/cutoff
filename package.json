{
  "name": "cutoff",
  "version": "0.0.13",
  "description": "An opinionated command line utility for cutting releases and publishing to npm.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bad-batch/cutoff",
  "bugs": {
    "url": "https://github.com/bad-batch/cutoff/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bad-batch/cutoff"
  },
  "keywords": [
    "command line",
    "cli",
    "lerna",
    "npm",
    "releases"
  ],
  "bin": {
    "cutoff": "bin/cutoff.js",
    "cutoff-lerna": "bin/cutoff-lerna.js",
    "publish-cutoff": "bin/publish-cutoff.js",
    "publish-lerna-cutoff": "bin/publish-lerna-cutoff.js",
    "publish-lerna-cutoff-pkg": "bin/publish-lerna-cutoff-pkg.js"
  },
  "scripts": {
    "build": "yarn run clean && yarn run test && yarn run compile",
    "changelog": "changelog",
    "clean": "del lib",
    "compile": "gulp compile",
    "cutoff": "node bin/cutoff.js",
    "lint": "yarn run tslint && yarn run markdownLint",
    "markdownLint": "markdownlint README.md",
    "publish-cutoff": "node bin/publish-cutoff.js",
    "test": "yarn run lint && yarn run type-check && yarn run test:node",
    "test:node": "cross-env TS_NODE_PROJECT=\"./tsconfig.test.json\" nyc mocha --opts \"./mocha.opts\"",
    "tslint": "gulp tslint",
    "type-check": "gulp type-check"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run test"
    }
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.44",
    "dotenv": "^6.0.0",
    "generate-changelog": "^1.7.1",
    "semver": "^5.5.0",
    "shelljs": "^0.8.1",
    "yargs": "^11.0.0"
  },
  "peerDependencies": {
    "lerna": "^2.9.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-stage-0": "7.0.0-beta.44",
    "@types/chai": "^4.1.3",
    "@types/dotenv": "^4.0.3",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.2",
    "@types/semver": "^5.5.0",
    "@types/shelljs": "^0.8.0",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^2.7.32",
    "@types/yargs": "^11.0.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.6",
    "del-cli": "^1.1.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "husky": "^0.15.0-rc.13",
    "markdownlint-cli": "^0.10.0",
    "merge-stream": "^1.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.1.0",
    "tsconfig-paths": "^3.4.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
